{
  "name": "Angular2",
  "version": "0.2.0",
  "description": "Tutorial for learning Angular2 with TypeScript",
  "main": "main.js",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
